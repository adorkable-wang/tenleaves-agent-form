VITE_AGENT_ENDPOINT=/api/agent/analyze
VITE_AGENT_PROXY_TARGET=http://localhost:8787

# 可选：如果后端需要鉴权，可以配置 API Key（前端会作为 Bearer Token 发送）
# VITE_AGENT_API_KEY=

# DashScope 配置（必填）
DASHSCOPE_API_KEY=your_dashscope_api_key
DASHSCOPE_ENDPOINT=https://dashscope.aliyuncs.com/compatible-mode/v1/chat/completions
DASHSCOPE_MODEL=qwen-turbo

# LLM 请求超时与重试
LLM_TIMEOUT_ENABLED=false
LLM_TIMEOUT_MS=20000
LLM_RETRIES=1

# 可选：OpenAI 兼容接口 Key（仅作为其他实验用途）
# 建议按需单独创建 .env.openai，再在脚本中引用
OPENAI_API_KEY=your_openai_api_key
# OPENAI_MODEL=gpt-4.1-mini
