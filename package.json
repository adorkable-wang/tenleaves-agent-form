{
  "name": "tenleaves-agent-form",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:server": "tsx server/index.ts",
    "dev:all": "bash -c 'trap \"kill 0\" EXIT; pnpm run dev:server & pnpm run dev'",
    "build": "tsc -b && vite build",
    "build:types": "tsc -p tsconfig.lib.json",
    "build:lib": "vite build -c vite.lib.config.ts",
    "build:pkg": "pnpm run build:types && pnpm run build:lib",
    "lint": "eslint .",
    "typecheck": "tsc -p tsconfig.app.json --noEmit && tsc -p tsconfig.node.json --noEmit && tsc -p tsconfig.lib.json --noEmit",
    "preview": "vite preview",
    "agent:server": "tsx server/index.ts",
    "feature:update": "node server/update-feature-status.mjs"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "zod": "^3.23.8",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "mammoth": "^1.6.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/node": "^24.6.0",
    "@types/react": "^19.1.16",
    "@types/react-dom": "^19.1.9",
    "@unocss/reset": "^66.5.4",
    "@vitejs/plugin-react-swc": "^4.1.0",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.22",
    "globals": "^16.4.0",
    "tsx": "^4.19.2",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.45.0",
    "unocss": "^66.5.4",
    "vite": "^7.1.7"
  },
  "main": "dist-lib/tenleaves-agent-form.cjs",
  "module": "dist-lib/tenleaves-agent-form.es.js",
  "types": "dist-lib/types/index.d.ts",
  "sideEffects": false,
  "files": [
    "dist-lib"
  ],
  "exports": {
    ".": {
      "types": "./dist-lib/types/index.d.ts",
      "import": "./dist-lib/tenleaves-agent-form.es.js",
      "require": "./dist-lib/tenleaves-agent-form.cjs"
    }
  }
}
